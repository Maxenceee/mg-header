{
  "name": "mg-header",
  "displayName": "MG Header",
  "description": "Header",
  "version": "0.2.0",
  "publisher": "maxencegama",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mg-header.printheader"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mg-header.printheader",
        "title": "MG Header: Print Header"
      }
    ],
    "keybindings": [
      {
        "command": "mg-header.printheader",
        "key": "ctrl+alt+g",
        "mac": "cmd+alt+g",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.86.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  }
}
