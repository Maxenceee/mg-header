{
  "name": "mg-header",
  "displayName": "MG Header",
  "description": "Header",
  "version": "0.2.0",
  "publisher": "maxencegama",
  "engines": {
    "vscode": "^1.86.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Maxenceee/mg-header"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mg-header.printheader"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mg-header.printheader",
        "title": "MG Header: Print Header"
      }
    ],
    "keybindings": [
      {
        "command": "mg-header.printheader",
        "key": "ctrl+alt+g",
        "mac": "cmd+alt+g",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "@vscode/vsce": "^2.23.0"
  }
}
